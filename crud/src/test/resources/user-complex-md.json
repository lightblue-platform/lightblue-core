            "migrationDate": {
                "type": "date",
                "valueGenerator": {
                    "type": "CurrentTime"
                },
                "access": {
                    "insert": [
                        "lb-migratefields-insert"
                    ],
                    "update": [
                        "lb-migratefields-update"
                    ]
                }
            },
            "migratedFrom": {
                "type": "string",
                "access": {
                    "update": [
                        "lb-migratefields-insert"
                    ],
                    "update": [
                        "lb-migratefields-update"
