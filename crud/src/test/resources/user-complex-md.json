            },
            "status": {
                "type": "string",
                "constraints": {
                    "enum": "status",
                    "required": true
                }
            },
            "_id": {
                "type": "integer",
                "constraints": {
                    "identity": true
                },
                "valueGenerator": {
                    "type": "IntSequence",
                    "configuration": {
                        "initialValue": "50000000",
                        "name": "userIdIntSequence"
                    }
                },
                "description": "Numeric id required by legacy user service and model.",
                "access": {
                    "update": [
                        "noone"
                    ]
                }
            },
            "simpleUserId": {
                "type": "integer",
                "description": "Numeric id required by legacy simple user service and model.",
                "access": {
                    "insert": [
